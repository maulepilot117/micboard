{
  "name": "micboard",
  "version": "0.8.5",
  "description": "An intelligent mic tray for ethernet enabled Shure microphones.",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "server": "python py/micboard.py",
    "binary": "pyinstaller py/micboard.spec",
    "build:legacy": "webpack --progress --config=webpack.config.js",
    "build:react": "tsc && vite build",
    "build": "npm run build:react",
    "preview": "vite preview",
    "app": "electron .",
    "pack": "node electron-build.js",
    "dist": "electron-builder",
    "static": "python -m http.server 8058"
  },
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karlcswanson/micboard.git"
  },
  "author": "karlcswanson@gmail.com",
  "bugs": {
    "url": "https://github.com/karlcswanson/micboard/issues"
  },
  "homepage": "https://github.com/karlcswanson/micboard",
  "dependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@ibm/plex": "^6.4.1",
    "@popperjs/core": "^2.11.8",
    "@shopify/draggable": "^1.1.3",
    "@tanstack/react-query": "^5.12.2",
    "babel-loader": "^9.1.3",
    "bootstrap": "^5.3.3",
    "core-js": "^3.36.0",
    "css-loader": "^6.10.0",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "style-loader": "^3.3.4",
    "uplot": "^1.6.32",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.16",
    "electron": "^32.2.0",
    "electron-builder": "^25.0.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  }
