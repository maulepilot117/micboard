    "@shopify/draggable": "^1.0.0-beta.8",
    "babel-loader": "^9.1.3",
    "bootstrap": "^4.3.1",
    "core-js": "^3.36.0",
    "css-loader": "^6.10.0",
    "jquery": "^3.4.1",
    "node-gyp": "^4.0.0",
